	Month = nov,
	Number = {4},
	Pages = {1523--1543},
	Shorttitle = {{\textbackslash}textit\{{Colloquium}\}},
	Title = {{\textbackslash}textit\{{Colloquium}\} : {Artificial} gauge potentials for neutral atoms},
	Url = {http://link.aps.org/doi/10.1103/RevModPhys.83.1523},
